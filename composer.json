{
    "name": "bartvantuijn/ventonbinary",
    "description": "A fast and lightweight developer CLI built with Laravel Zero, designed to simplify local workflows, manage Docker environments, and automate repetitive developer tasks",
    "type": "project",
    "keywords": ["ventonbinary", "laravel", "docker"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bart van Tuijn",
            "email": "development@venton.systems"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "laravel-zero/framework": "^11.0.2"
    },
    "require-dev": {
        "laravel/pint": "^1.18.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.5.1",
        "rector/rector": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": [
            "rector",
            "pint"
        ],
        "pint": "pint --config pint.json",
        "rector": "rector"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["venton"]
}
